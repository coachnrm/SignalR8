@model List<SignalR8.Models.NurseRequest>

@{
}


<table class="table">
    <thead>
        <tr>
            <th>QN</th>
            <th>ชื่อ-นามสกุล</th>
            <th>ต้นทาง</th>
            <th>ปลายทาง</th>
            <th>พนักงานเวรเปล</th>
            <th>ประเภทรถ</th>
            <th>ระดับความเร่งด่วน </th>
            <th>สถานะ</th>
            <th>หมายเหตุ</th>
            <th>ยืนยัน</th>
            <th>แก้ไข</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var nurserequest in Model)
        {
            <tr>
                <td>@nurserequest.QN</td>
                <td>@nurserequest.QNName</td>
                <td>@nurserequest.QNAge</td>
                <td>@nurserequest.QNSex</td>
                <td>@nurserequest.Department</td>
                <td>@nurserequest.StartPoint</td>
                <td>@nurserequest.EndPoint</td>
                <td>@nurserequest.PoterFname</td>
                <td>@nurserequest.MaterialType</td>
                <td>@nurserequest.UrentType</td>
                <td>@nurserequest.Remark</td>
                <td><a href="View/@nurserequest.JobId">View</a></td>
            </tr>
        }
    </tbody>
</table>
